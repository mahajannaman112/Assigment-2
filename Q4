//part A: concat two strings
s1 = "Hello"
s2 = "World"
s3 = s1 + s2
print("Concatenated:", s3)

//part B: reverse a string
txt = "hello"
rev = txt[::-1]
print("Reversed:", rev)

//part c: delete all vowels
st = "Hello World"
vowels = "aeiouAEIOU"
res = ""
for ch in st:
    if ch not in vowels:
        res += ch
print("No vowels:", res)

//part d:sort strings alphabetically
words = ["mango", "grapes", "banana", "apple","guava"]
words.sort()
print("Sorted:", words)

//part e: upper to lower
char = "A"
low = char.lower()
print("Lowercase:", low)
